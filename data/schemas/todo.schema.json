{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "todo.schema.json",
    "title": "todo",
    "description": "Todo",
    "type": "object",
    "properties": {
        "id": {
            "type": "number",
            "exclusiveMinimum": 0,
            "uniqueProperty": true
        },
        "userId": {
            "type": "number",
            "exclusiveMinimum": 0
        },
        "title": {
            "type": "string"
        },
        "completed": {
            "type": "boolean"
        }
    },
    "required": [
        "id",
        "userId",
        "title",
        "completed"
    ]
}