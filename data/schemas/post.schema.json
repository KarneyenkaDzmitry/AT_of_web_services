{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "post.schema.json",
    "title": "post",
    "description": "Post",
    "type": "object",
    "properties": {
        "id": {
            "type": "number",
            "exclusiveMinimum": 0,
            "uniqueProperty": true
        },
        "userId": {
            "type": "number",
            "exclusiveMinimum": 0
        },
        "title": {
            "type": "string"
        },
        "body": {
            "type": "string"
        }
    },
    "required": [
        "id",
        "userId",
        "title",
        "body"
    ]
}