{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "user.schema.json",
    "title": "user data",
    "description": "User data",
    "type": "object",
    "properties": {
        "id": {
            "type": "number",
            "exclusiveMinimum": 0
        },
        "name": {
            "type": "string"
        },
        "username": {
            "type": "string"
        },
        "email": {
            "type": "string"
        },
        "address": {
            "type": "object"
        }
    },
    "required": [
        "id",
        "name",
        "username",
        "email",
        "address"
    ]
}