{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "photo.schema.json",
    "title": "photo",
    "description": "Photo",
    "type": "object",
    "properties": {
        "id": {
            "type": "number",
            "exclusiveMinimum": 0,
            "uniqueProperty": true
        },
        "albumId": {
            "type": "number",
            "exclusiveMinimum": 0
        },
        "title": {
            "type": "string"
        },
        "url": {
            "type": "string"
        },
        "thumbnailUrl": {
            "type": "string"
        }
    },
    "required": [
        "id",
        "albumId",
        "title",
        "url",
        "thumbnailUrl"
    ]
}